<div class="row firsts">
    <div class="col-lg-4 col-md-4 col-sm-12">
        <div class="links">
            <div class="link">
                <i class="fa fa-facebook" aria-hidden="true"></i>
            </div>
            <div class="link">
                <i class="fa fa-instagram" aria-hidden="true"></i>
            </div>
            <div class="link">
                <i class="fa fa-twitter" aria-hidden="true"></i>
            </div>
            <div class="link">
                <img src="/images/main/telegram.png" alt="telegram">
            </div>
            <div class="link">
                <img src="/images/main/tik-tok.png" alt="tik-tok">
            </div>
        </div>
    </div>
    
    <div class="col-lg-4 col-md-4 col-sm-12 d-flex justify-content-center">
        <div class="logo">
            <img src="/images/main/logo.png" alt="Logo">
        </div>
    </div>
    
    <div class="col-lg-4 col-md-4 d-flex justify-content-end listss">
        <ul class="list p-0">
            <li><a href="/news">Yangiliklar</a></li>
            <li><a href="/blog">Blog</a></li>
            <li><a href="/aloqa">Aloqa</a></li>
        </ul>
    </div>
</div>
<div class="row seconds d-flex align-items-start start">
    <div class="col-lg-4 col-md-4 col-sm-4 d-flex justify-content-end end">
        <div class="contact">
            <i class="fa fa-envelope" aria-hidden="true"></i>
            <a href="mailto:"><%= info.mail %></a>
        </div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-4 d-flex justify-content-center end">
        <div class="contact">
            <i class="fa fa-volume-control-phone" aria-hidden="true"></i>
            <p class="p-0 m-0">+<%= info.number1 %></p>
            <p class="p-0 m-0">+<%= info.number2 %></p>
        </div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-4 d-flex justify-content-start end">
        <div class="contact">
            <i class="fa fa-map-marker" aria-hidden="true"></i>
            <p><%= info.address %></p>
        </div>
    </div>
</div>
