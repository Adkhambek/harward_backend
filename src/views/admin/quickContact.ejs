<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <meta name="author" content="Bdtask">
    <title>Quick Contacts</title>
    <!--Global Styles(used by all pages)-->
    <link href="/admin/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="/admin/plugins/metisMenu/metisMenu.min.css" rel="stylesheet">
    <link href="/admin/plugins/fontawesome/css/all.min.css" rel="stylesheet">
    <link href="/admin/plugins/typicons/src/typicons.min.css" rel="stylesheet">
    <link href="/admin/plugins/themify-icons/themify-icons.min.css" rel="stylesheet">
    <!--Third party Styles(used by this page)-->
    <link href="/admin/plugins/datatables/dataTables.bootstrap4.min.css" 
    rel="stylesheet">
    <!--Start Your Custom Style Now-->
    <link href="/admin/dist/css/style.css" rel="stylesheet">
</head>
<body>
    <!-- Page loader -->
    <%- include("includes/loader.ejs")%>

    <div class="wrapper">
        <!-- Sidebar  -->
        <%- include("includes/sidebar.ejs", {page: "quick-contacts"}) %>

        <div class="content-wrapper">
            <div class="main-content">
                <!-- Header -->
                <%- include("includes/header.ejs")%>
                <div class="content-header row align-items-center m-0">
                    <nav aria-label="breadcrumb" class="col-sm-4 order-sm-last mb-3 mb-sm-0 p-0 ">
                        <ol class="breadcrumb d-inline-flex font-weight-600 fs-13 bg-white mb-0 float-sm-right">
                            <% for(const item of breadcrumb) {%>
                                <% if(breadcrumb.indexOf(item) === breadcrumb.length - 1) {%>
                                    <li class="breadcrumb-item active"><%= item.name %></li>
                                <%} else {%>
                                    <li class="breadcrumb-item"><a href="<%= item.url %>"><%= item.name %></a></li>
                                <%}%>   
                            <%}%>
                        </ol>
                    </nav>
                    <div class="col-sm-8 header-title p-0">
                        <div class="media">
                            <div class="header-icon text-success mr-3"><i class="typcn typcn-puzzle-outline"></i></div>
                            <div class="media-body">
                                <h1 class="font-weight-bold">Quick contacts</h1>
                                <small>In this page, you can see all quick contacts</small>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="body-content">
                    <% if(successMessage != "") { %>
                        <div class="alert alert-success alert-dismissible fade show" role="alert">
                            <%= successMessage %>
                            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                              </button>
                          </div>
                        <% } %>
                    <div class="card mb-4">
                        <div class="card-header">
                            <div class="d-flex justify-content-between align-items-center">
                                <div>
                                    <h6 class="fs-17 font-weight-600 mb-0">Unchecked contacts</h6>
                                </div>

                            </div>
                        </div>
                        <div class="card-body">
                   <div class="table-responsive">
                    <table class="table display text-center table-bordered table-striped basic">
                        <thead>
                            
                            <tr>
                                <th>Number</th>
                                <th>Name</th>
                                <th>Tel</th>
                                <th>Date</th>
                                <th>Action</th>
                            </tr>
                            
                        </thead>
                        <tbody>
                            <% let i = 0 %>
                            <% for(item of contacts){%>
                            <tr>
                                <td><%= ++i %></td>
                                <td><%= item.name %></td>
                                <td><%= item.number %></td>
                                <td><%= item.time %></td>
                                <td>
                                    <a href="/admin/quick-contacts/checked/<%= item.home_contact_id%>" class="btn btn-success">
                                        <span class="fas fa-check" ></span>
                                    </a>
                                </td>
                            </tr>
                            <% } %>
                        </tbody>
                    </table> 
                   
                    <div class="md-overlay"></div>
                   </div>
                </div>
                    </div>
                    <div class="card mb-4">
                        <div class="card-header">
                            <div class="d-flex justify-content-between align-items-center">
                                <div>
                                    <h6 class="fs-17 font-weight-600 mb-0">Checked contacts</h6>
                                </div>

                            </div>
                        </div>
                        <div class="card-body">
                   <div class="table-responsive">
                    <table class="table display text-center table-bordered table-striped basic">
                        <thead>
                            
                            <tr>
                                <th>Number</th>
                                <th>Name</th>
                                <th>Tel</th>
                                <th>Date</th>
                            </tr>
                            
                        </thead>
                        <tbody>
                            <% let k = 0 %>
                            <% for(item of checkedContacts){%>
                            <tr>
                                <td><%= ++k %></td>
                                <td><%= item.name %></td>
                                <td><%= item.number %></td>
                                <td><%= item.time %></td>
                            </tr>
                            <% } %>
                        </tbody>
                    </table> 
                </div>
                </div>
            </div>
                        <div class="md-overlay"></div>
                </div>
            </div>

        </div>
    </div>
    </div>

    </div>
    
    <div class="overlay"></div>
    </div>

    </div>

    <!--Global script(used by all pages)-->
    <script data-cfasync="false" src="cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
    <script src="/admin/plugins/jQuery/jquery-3.4.1.min.js"></script>
    <script src="/admin/dist/js/popper.min.js"></script>
    <script src="/admin/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="/admin/plugins/metisMenu/metisMenu.min.js"></script>
    <script src="/admin/plugins/perfect-scrollbar/dist/perfect-scrollbar.min.js"></script>

    <!-- Third Party Scripts(used by this page)-->
    <script src="/admin/plugins/datatables/dataTables.min.js"></script>
    <script src="/admin/plugins/datatables/dataTables.bootstrap4.min.js"></script>
    <!--Page Active Scripts(used by this page)-->
    <script src="/admin/plugins/datatables/data-basic.active.js"></script>
    </section>
     <script>
      $(document).ready(function() {
    $('#example').DataTable({

        responsive: true,
        'columnDefs': [{
            'orderable': false,
        }]

    });

} );
      </script>
    <script src="/admin/dist/js/sidebar.js"></script>
</body>

</html>