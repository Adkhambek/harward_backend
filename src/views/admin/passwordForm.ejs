<!DOCTYPE html>
<html lang="en">
<head>
	<!-- Required meta tags -->
	<meta charset="utf-8" />
	<meta
	name="viewport"
	content="width=device-width, initial-scale=1, shrink-to-fit=no"
	/>
	<meta
	name="description"
	content="Responsive Bootstrap 4 Admin &amp; Dashboard Template"
	/>
	<meta name="author" content="Bdtask" />
	<title>Main Page</title>
	<!--Global Styles(used by all pages)-->
	<link
	href="/admin/plugins/bootstrap/css/bootstrap.min.css"
	rel="stylesheet"
	/>
	<link href="/admin/plugins/metisMenu/metisMenu.min.css" rel="stylesheet" />
	<link href="/admin/plugins/fontawesome/css/all.min.css" rel="stylesheet" />
	<link
	href="/admin/plugins/typicons/src/typicons.min.css"
	rel="stylesheet"
	/>
	<link
	href="/admin/plugins/themify-icons/themify-icons.min.css"
	rel="stylesheet"
	/>
	<!--Start Your Custom Style Now-->
	<link href="/admin/dist/css/style.css" rel="stylesheet" />
</head>

<body>
	<!-- Page loader -->
	<%- include("includes/loader.ejs")%>
	
	<div class="wrapper">
		<!-- Sidebar  -->
		<%- include("includes/sidebar.ejs") %>
		
		<div class="content-wrapper">
			<div class="main-content">
				<!-- Header -->
				<%- include("includes/header.ejs")%>
				
				<div class="body-content">
					<form action="/admin/password" method="POST">
						<div class="form-group">
							<label for="exampleInputEmail1" class="font-weight-600">
								Old password:
							</label>
							<input name="oldPassword" type="password" class="form-control mt-2" required>
						</div>
						<div class="form-group">
							<label for="exampleInputEmail1" class="font-weight-600">
								New password:
							</label>
							<input name="newPassword" type="password" class="form-control mt-2" required>
						</div>
						<div class="form-group">
							<label for="exampleInputEmail1" class="font-weight-600">
								Confirm new password:
							</label>
							<input name="confirmPassword" type="password" class="form-control mt-2" required>
						</div>
						<button type="submit" class="btn btn-primary mt-5">
							Publish
						</button>
					</form>
				</div>
			</div>
		
		<div class="overlay"></div>
	</div>
</div>

<!--Global script(used by all pages)-->
<script
data-cfasync="false"
src="cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"
></script>
<script src="/admin/plugins/jQuery/jquery-3.4.1.min.js"></script>
<script src="/admin/dist/js/popper.min.js"></script>
<script src="/admin/plugins/bootstrap/js/bootstrap.min.js"></script>
<script src="/admin/plugins/metisMenu/metisMenu.min.js"></script>
<script src="/admin/plugins/perfect-scrollbar/dist/perfect-scrollbar.min.js"></script>
<script src="/admin/dist/js/sidebar.js"></script>
</body>
</html>
